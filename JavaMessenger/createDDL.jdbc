CREATE TABLE users (id BIGINT AUTO_INCREMENT NOT NULL, user_name VARCHAR(255), ip VARCHAR(255), password VARCHAR(255), PRIMARY KEY (id))
CREATE TABLE user_to_conference (id BIGINT AUTO_INCREMENT NOT NULL, user_id INTEGER, conference_id INTEGER, PRIMARY KEY (id))
CREATE TABLE conferences (id BIGINT AUTO_INCREMENT NOT NULL, message_id INTEGER, PRIMARY KEY (id))
CREATE TABLE messages (id BIGINT AUTO_INCREMENT NOT NULL, content VARCHAR(255), timestamp DATETIME, sender_id VARCHAR(255), PRIMARY KEY (id))
CREATE TABLE contacts (id BIGINT AUTO_INCREMENT NOT NULL, user_id INTEGER, number INTEGER, name VARCHAR(255), PRIMARY KEY (id))
CREATE TABLE USER (ID BIGINT NOT NULL, user_name VARCHAR(255), ip VARCHAR(255), password VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(38), PRIMARY KEY (SEQ_NAME))
